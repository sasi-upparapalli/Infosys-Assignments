a = 10
b = -5
c = 0b1010
d = 0o12
e = 0xA
f = 10.5
g = 3 + 4j
h = True
i = False

print(a, type(a))   # 10 <class 'int'>
print(b, type(b))   # -5 <class 'int'>
print(c, type(c))   # 10 <class 'int'>
print(d, type(d))   # 10 <class 'int'>
print(e, type(e))   # 10 <class 'int'>
print(f, type(f))   # 10.5 <class 'float'>
print(g, type(g))   # (3+4j) <class 'complex'>
print(h, type(h))   # True <class 'bool'>
print(i, type(i))   # False <class 'bool'>
